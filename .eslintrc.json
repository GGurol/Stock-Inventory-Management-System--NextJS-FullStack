{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "react-hooks/exhaustive-deps": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^handleLogin$|^params$|^searchParams$|nanoid|MdInventory|MdBuild|MdPhoneIphone|MdLaptop|MdKitchen|MdHome|MdChair|MdLightbulbCircle|MdOutlineDoorSliding|FaBed|FaHeadphones|FaTv|IoTabletPortrait|GiSofa",
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true,
        "project": "./tsconfig.json"
      }
    }
  }
}
